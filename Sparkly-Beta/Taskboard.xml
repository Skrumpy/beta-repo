<?xml version="1.0" encoding="utf-8"?>
<Container>
<Name>Taskboard</Name>
<support>http://lime-technology.com/forum/index.php?topic=38232</support>
<Description>A simple, visual way to keep track of what needs to get done[br][b]IMPORTANT:- You must change the host port below to avoid clash with default unraid webui port.[/b][br]More information http://taskboard.matthewross.me/</Description>
<Registry>https://registry.hub.docker.com/u/sparklyballs/taskboard/</Registry>
<Repository>sparklyballs/taskboard</Repository>
<BindTime>true</BindTime>
<Privileged>false</Privileged>
<Networking>
<Mode>bridge</Mode>
<Publish>
<Port>
<HostPort>80</HostPort>
<ContainerPort>80</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
</Publish>
</Networking>
<Environment>
<Variable>
<Name></Name>
<Value></Value>
</Variable>
</Environment>
<Data>
<Volume>
<HostDir></HostDir>
<ContainerDir>/var/www/TaskBoard</ContainerDir>
<Mode>rw</Mode>
</Volume>
<Volume>
<HostDir></HostDir>
<ContainerDir></ContainerDir>
<Mode>rw</Mode>
</Volume>
</Data>
<WebUI>http://[IP]:[PORT:80]/</WebUI>
<Banner></Banner>
<Icon>http://emotionalsurvivalkit.com.au/wp-content/post-its/post-it%20note6.png</Icon>
</Container>
