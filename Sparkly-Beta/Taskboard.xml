<Volume>
<HostDir></HostDir>
<ContainerDir>/var/www/TaskBoard</ContainerDir>
<Mode>rw</Mode>
</Volume>
<Volume>
<HostDir></HostDir>
<ContainerDir></ContainerDir>
<Mode>rw</Mode>
</Volume>
</Data>
<WebUI>http://[IP]:[PORT:80]/</WebUI>
<Banner></Banner>
