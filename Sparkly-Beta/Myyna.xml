<?xml version="1.0" encoding="utf-8"?>
<Container>
<Name>Baikal</Name>
<Description>Myyna test installation only[br]IMPORTANT :- YOU MUST REGISTER AT http://www.myyna.com/[br] USE LOCALHOST AND LEAVE USER/PASSWORD BLANK FOR DATABASE</Description>
<Registry>https://registry.hub.docker.com/u/sparklyballs/baikal/</Registry>
<Repository>sparklyballs/baikal</Repository>
<BindTime>true</BindTime>
<Privileged>false</Privileged>
<Networking>
<Mode>bridge</Mode>
<Publish>
<Port>
<HostPort>3000</HostPort>
<ContainerPort>3000</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
</Publish>
</Networking>
<Environment>
<Variable>
<Name></Name>
<Value></Value>
</Variable>
</Environment>
<Data>
<Volume>
<HostDir></HostDir>
<ContainerDir>/var/www/mynna</ContainerDir>
<Mode>rw</Mode>
</Volume>
<Volume>
<HostDir></HostDir>
<ContainerDir>/data/db</ContainerDir>
<Mode>rw</Mode>
</Volume>
</Data>
<WebUI>http://[IP]:[PORT:3000]/</WebUI>
<Banner>http://www.myyna.com/wp-content/uploads/2014/02/band_logo11.png</Banner>
<Icon>http://i.imgur.com/eQ3WXaV.png</Icon>
</Container>
