<?xml version="1.0" encoding="utf-8"?>
<Container>
<Name>Shout-Irc</Name>
<Overview>Browser based IRC client using node</Overview>
<Support>http://lime-technology.com/forum/index.php?topic=38232</Support>
<Description>Browser based IRC client using node[br][br]Config options are contained in config.js in the mapped volume.</Description>
<Registry>https://registry.hub.docker.com/u/sparklyballs/shout-irc/</Registry>
<Repository>sparklyballs/shout-irc</Repository>
<BindTime>true</BindTime>
<Privileged>false</Privileged>
<Networking>
<Mode>bridge</Mode>
<Publish>
<Port>
<HostPort>9000</HostPort>
<ContainerPort>9000</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
</Publish>
</Networking>
<Environment>
<Variable>
<Name></Name>
<Value></Value>
</Variable>
</Environment>
<Data>
<Volume>
<HostDir>/usr/local/lib/node_modules/shout</HostDir>
<ContainerDir></ContainerDir>
<Mode></Mode>
</Volume>
<Volume>
<HostDir></HostDir>
<ContainerDir></ContainerDir>
<Mode></Mode>
</Volume>
</Data>
<WebUI>http://[IP]:[PORT:9000]/</WebUI>
<Banner>http://i.imgur.com/29Q6GZl.png</Banner>
<Icon>http://i.imgur.com/hEVCx8x.png</Icon>
</Container>
