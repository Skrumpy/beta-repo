<?xml version="1.0" encoding="utf-8"?>
<Container>
<Name>Pf-Logstash</Name>
<Description>Pfsense logstash test installation only</Description>
<Registry>https://registry.hub.docker.com/u/sparklyballs/elkstash-pfsense/</Registry>
<Repository>sparklyballs/elkstash-pfsense</Repository>
<BindTime>true</BindTime>
<Privileged>false</Privileged>
<Networking>
<Mode>host</Mode>
<Publish>
<Port>
<HostPort>8092</HostPort>
<ContainerPort>8092</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
<Port>
<HostPort>9200</HostPort>
<ContainerPort>9200</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
<Port>
<HostPort>5140</HostPort>
<ContainerPort>5140</ContainerPort>
<Protocol>udp</Protocol>
</Port>
</Publish>
</Networking>
<Environment>
<Variable>
<Name></Name>
<Value></Value>
</Variable>
</Environment>
<Data>
<Volume>
<HostDir></HostDir>
<ContainerDir>/config</ContainerDir>
<Mode>rw</Mode>
</Volume>
</Data>
<WebUI>http://[IP]:[PORT:8092]/</WebUI>
<Banner>http://3.bp.blogspot.com/-kEJdMJOk-Ek/VLBP35BVfYI/AAAAAAACPBc/1NERucHXl_o/s1600/kibana_banner.png</Banner>
<Icon>http://i.imgur.com/Bkp9gC2.png</Icon>
</Container>
