<?xml version="1.0" encoding="utf-8"?>
<Container>
<Name>Pf-Logstash</Name>
<Description>ELK stack specifically for monitoring PFsense (2.2 by default)</Description>
<Registry>https://registry.hub.docker.com/u/sparklyballs/elkstash/</Registry>
<Repository>sparklyballs/elkstash</Repository>
<BindTime>true</BindTime>
<Privileged>false</Privileged>
<Networking>
<Mode>bridge</Mode>
<Publish>
<Port>
<HostPort>80</HostPort>
<ContainerPort>80</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
<Port>
<HostPort>9200</HostPort>
<ContainerPort>9200</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
<Port>
<HostPort>5140</HostPort>
<ContainerPort>5140</ContainerPort>
<Protocol>udp</Protocol>
</Port>
</Publish>
</Networking>
<Environment>
<Variable>
<Name></Name>
<Value></Value>
</Variable>
</Environment>
<Data>
<Volume>
<HostDir></HostDir>
<ContainerDir>/config</ContainerDir>
<Mode>rw</Mode>
</Volume>
<Volume>
<HostDir></HostDir>
<ContainerDir>/var/www/kibana</ContainerDir>
<Mode>rw</Mode>
</Volume>
</Data>
<WebUI>http://[IP]:[PORT:80]/index.html#/dashboard/file/pfsense.json</WebUI>
<Banner>http://3.bp.blogspot.com/-kEJdMJOk-Ek/VLBP35BVfYI/AAAAAAACPBc/1NERucHXl_o/s1600/kibana_banner.png</Banner>
<Icon>http://i.imgur.com/Bkp9gC2.png</Icon>
</Container>
