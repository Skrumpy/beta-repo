<?xml version="1.0" encoding="utf-8"?>
<Container>
<Name>SynaMan</Name>
<Description>Web-based remote file manager allowing users to upload or download files from any machine[br]Map volume /shared to your local files.</Description>
<Registry>https://registry.hub.docker.com/u/sparklyballs/synaman/</Registry>
<Repository>sparklyballs/synaman</Repository>
<BindTime>true</BindTime>
<Privileged>false</Privileged>
<Networking>
<Mode>host</Mode>
<Publish>
<Port>
<HostPort>6060</HostPort>
<ContainerPort>6060</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
</Publish>
</Networking>
<Environment>
<Variable>
<Name></Name>
<Value></Value>
</Variable>
</Environment>
<Data>
<Volume>
<HostDir></HostDir>
<ContainerDir>/opt/SynaMan</ContainerDir>
<Mode>rw</Mode>
</Volume>
<Volume>
<HostDir></HostDir>
<ContainerDir>/Shared</ContainerDir>
<Mode>rw</Mode>
</Volume>
</Data>
<WebUI>http://[IP]:[PORT:6060]/</WebUI>
<Banner></Banner>
<Icon>https://ftp.bernards.com/images/Login.png</Icon>
</Container>
