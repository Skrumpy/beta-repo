<?xml version="1.0" encoding="utf-8"?>
<Container>
<Name>Gerrit</Name>
<Description>Gerrit is a web based code review system, facilitating online code reviews for projects using the Git version control system.</Description>
<Registry>https://registry.hub.docker.com/u/sparklyballs/gerrit/</Registry>
<Repository>sparklyballs/gerrit</Repository>
<BindTime>true</BindTime>
<Privileged>false</Privileged>
<Networking>
<Mode>bridge</Mode>
<Publish>
<Port>
<HostPort>8080</HostPort>
<ContainerPort>8080</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
<Port>
<HostPort>29418</HostPort>
<ContainerPort>29418</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
</Publish>
</Networking>
<Environment>
<Variable>
<Name></Name>
<Value></Value>
</Variable>
</Environment>
<Data>
<Volume>
<HostDir></HostDir>
<ContainerDir>/opt/gerrit</ContainerDir>
<Mode>rw</Mode>
</Volume>
<Volume>
<HostDir></HostDir>
<ContainerDir></ContainerDir>
<Mode>rw</Mode>
</Volume>
</Data>
<WebUI>http://[IP]:[PORT:8080]/</WebUI>
<Banner></Banner>
<Icon>http://i.imgur.com/33067ek.png</Icon>
</Container>
