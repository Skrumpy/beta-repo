<?xml version="1.0" encoding="utf-8"?>
<Container>
<Name>ComicStreamer</Name>
<Description>Comicstreamer test installation only[br]/root/.ComicStreamer is config folder[br]/comics should be mapped to your local comics</Description>
<Registry>https://registry.hub.docker.com/u/sparklyballs/comicstreamer/</Registry>
<Repository>sparklyballs/comicstreamer</Repository>
<BindTime>true</BindTime>
<Privileged>false</Privileged>
<Networking>
<Mode>bridge</Mode>
<Publish>
<Port>
<HostPort>32500</HostPort>
<ContainerPort>32500</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
</Publish>
</Networking>
<Environment>
<Variable>
<Name></Name>
<Value></Value>
</Variable>
</Environment>
<Data>
<Volume>
<HostDir></HostDir>
<ContainerDir>/comics</ContainerDir>
<Mode>rw</Mode>
</Volume>
<Volume>
<HostDir></HostDir>
<ContainerDir>/root/.ComicStreamer</ContainerDir>
<Mode>rw</Mode>
</Volume>
</Data>
<WebUI>http://[IP]:[PORT:32500]/</WebUI>
<Banner></Banner>
<Icon>http://upload.wikimedia.org/wikipedia/commons/3/37/Simple_Manga.png</Icon>
</Container>
