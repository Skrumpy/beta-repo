<?xml version="1.0" encoding="utf-8"?>
<Container>
<Name>Freshrss</Name>
<Description>FreshRSS is a free, self hostable, rss aggregator[br][b]IMPORTANT :- YOU MUST CHANGE THE HOST PORT BELOW[/b]</Description>
<Registry>https://registry.hub.docker.com/u/sparklyballs/freshrss/</Registry>
<Repository>sparklyballs/freshrss</Repository>
<BindTime>true</BindTime>
<Privileged>false</Privileged>
<Networking>
<Mode>bridge</Mode>
<Publish>
<Port>
<HostPort>80</HostPort>
<ContainerPort>80</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
</Publish>
</Networking>
<Environment>
<Variable>
<Name></Name>
<Value></Value>
</Variable>
</Environment>
<Data>
<Volume>
<HostDir></HostDir>
<ContainerDir>/var/www/freshrss</ContainerDir>
<Mode>rw</Mode>
</Volume>
<Volume>
<HostDir></HostDir>
<ContainerDir></ContainerDir>
<Mode>rw</Mode>
</Volume>
</Data>
<WebUI>http://[IP]:[PORT:80]/</WebUI>
<Banner>http://www.mouserunner.net/free_graphics/RSS/RSS_Oval_Page_Preview.png</Banner>
<Icon>http://www.easi-ie.com/wp-content/uploads/2012/05/rss.png</Icon>
</Container>
