<?xml version="1.0" encoding="utf-8"?>
<Container>
<Name>Baikal</Name>
<Description>Baikal test installation only[br]IMPORTANT :- YOU MUST CHANGE THE HOST PORT BELOW[br] ALSO SELECT SQLITE AS DATABASE TYPE (DEFAULT)</Description>
<Registry>https://registry.hub.docker.com/u/sparklyballs/baikal/</Registry>
<Repository>sparklyballs/lychee</Repository>
<BindTime>true</BindTime>
<Privileged>false</Privileged>
<Networking>
<Mode>bridge</Mode>
<Publish>
<Port>
<HostPort>80</HostPort>
<ContainerPort>80</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
</Publish>
</Networking>
<Environment>
<Variable>
<Name></Name>
<Value></Value>
</Variable>
</Environment>
<Data>
<Volume>
<HostDir></HostDir>
<ContainerDir>/var/www/baikal-regular</ContainerDir>
<Mode>rw</Mode>
</Volume>
<Volume>
<HostDir></HostDir>
<ContainerDir></ContainerDir>
<Mode>rw</Mode>
</Volume>
</Data>
<WebUI>http://[IP]:[PORT:80]/</WebUI>
<Banner></Banner>
<Icon>http://busymac.com/images/icon.png</Icon>
</Container>
