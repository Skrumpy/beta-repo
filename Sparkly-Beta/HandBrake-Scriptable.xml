<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>HandBrake</Name>
  <Support>http://lime-technology.com/forum/index.php?topic=39624</Support>
  <Description>Scriptable version of handbrake, with both the GUI and CLI version of HB.[br][br][b]Map Movies folder for movies you want to edit in the GUI, Map watch folder for movies you want to be processed by script automatically, map script folder for placing script in.</Description>
  <Overview>Scriptable version of handbrake, with both the GUI and CLI version of HB.</Overview>
  <Registry>https://registry.hub.docker.com/u/sparklyballs/handbrake-scriptable/</Registry>
  <Repository>sparklyballs/handbrake-scriptable</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8080</HostPort>
        <ContainerPort>8080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/Movies</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
       <Volume>
      <HostDir></HostDir>
      <ContainerDir>/Output</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/watch-folder</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
        <Volume>
      <HostDir></HostDir>
      <ContainerDir>/script</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
        <Volume>
      <HostDir></HostDir>
      <ContainerDir>/nobody/.config/ghb</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:8080]/#/client/c/HandBrake</WebUI>
  <Banner>http://i.imgur.com/ltUYNC2.png</Banner>
  <Icon>http://i.imgur.com/vTZfMnX.png</Icon>
</Container>
